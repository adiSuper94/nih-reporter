{
  "name": "@adisuper94/nih-reporter",
  "version": "0.0.16",
  "description": "A TypeScript/JavaScript wrapper to call NIH reporter API",
  "keywords": [
    "api",
    "nih",
    "reporter"
  ],
  "homepage": "https://github.com/adiSuper94/nih-reporter#readme",
  "bugs": {
    "url": "https://github.com/adiSuper94/nih-reporter/issues"
  },
  "license": "ISC",
  "author": "Aditya Subramanian",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adiSuper94/nih-reporter.git"
  },
  "type": "module",
  "main": "./dist/lib/index.js",
  "exports": {
    "./effect": {
      "types": "./dist/lib/effect/index.d.ts",
      "default": "./dist/lib/effect/index.js"
    },
    ".": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    }
  },
  "scripts": {
    "prepare": "effect-language-service patch",
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "tsc && node --test dist/tests/*.test.js",
    "check": "tsc --noEmit",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix"
  },
  "dependencies": {
    "@effect/platform": "^0.94.4",
    "effect": "^3.19.16",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@effect/language-service": "^0.73.0",
    "@types/node": "^24.0.0",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "typescript": "^5.9.3"
  }
}
